{
  "name": "@rethink_utaf/rethink_utaf",
  "version": "1.0.0",
  "description": "E2E Test Automation Framework setup for Cross Platforms, Web, Mobile and API. Current specific to Mobile, can be expanded to web when/if needed",
  "private": true,
  "scripts": {
    "allure:open": "allure open packages/**/allure/report",
    "allure:history": "mkdir -p packages/**/allure/report && cp -a packages/**/tmp/reports/allure/ packages/**/allure/.",
    "clean": "npx lerna run clean --parallel && npx shx rm -rf node_modules",
    "hello": "npx lerna run hello --parallel",
    "postinstall": "husky install",
    "lint": "npm run tsc && npx cross-env npx eslint . --ext .json,.eslintrc,.eslintignore,.js,.ts --fix && npx cross-env npx gherkin-lint -c .lint.feature -i **/node_modules/**/*.feature && npm run prettier",
    "prettier": "cross-env npx prettier --config .prettierrc --write '**/*.ts'",
    "setup": "npm install --legacy-peer-deps --hoist",
    "setup:appiumiOS": "globals/mobile/setup-appium-mac.sh",
    "tsc": "npx cross-env npx tsc -p tsconfig.json"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/config-lerna-scopes": "20.0.0",
    "@commitlint/prompt-cli": "^20.1.0",
    "@cucumber/cucumber": "^12.2.0",
    "@faker-js/faker": "^10.1.0",
    "@types/app-root-path": "^3.1.0",
    "@types/dateformat": "^5.0.3",
    "@types/dotenv-safe": "^9.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@wdio/allure-reporter": "^9.20.0",
    "@wdio/appium-service": "^9.20.0",
    "@wdio/browserstack-service": "^9.20.0",
    "@wdio/cli": "^9.20.0",
    "@wdio/cucumber-framework": "^9.20.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/shared-store-service": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "allure-commandline": "^2.34.1",
    "app-root-path": "^3.1.0",
    "appium": "^3.1.0",
    "appium-inspector-plugin": "^2025.8.2",
    "appium-uiautomator2-driver": "^4.0.0",
    "appium-xcuitest-driver": "^10.2.2",
    "axios": "^1.6.0",
    "commitizen": "^4.3.1",
    "cross-env": "^10.1.0",
    "cucumber-html-reporter": "^7.2.0",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "dotenv-safe": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "expect-webdriverio": "^5.4.3",
    "gherkin-lint": "^4.2.4",
    "husky": "^9.1.7",
    "lerna": "^9.0.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
